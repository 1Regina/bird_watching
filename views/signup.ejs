  <body>
    <%- include('navigator_bar') -%>
       <div class="col-sm-8 text-dark">
          <h3>Please join us in our efforts and join as a member</h3><br><br>

        <form action="/signup" method="POST">
        <label><h4><%= "Name" %></h4></label><br>  
        <input type="text" name="name" placeholder="your name"/><br><br>
        <label><h4><%= "Email Address" %></h4></label><br>  
        <input type="text" name="email" placeholder="your email contact"/><br><br>
        <label><h4><%= "Password" %></h4></label><br>  
        <input type="password" name="password" placeholder="password" /><br><br>
        <!-- <input type="submit" value="Submit"> -->
        <button type="submit" class="btn btn-secondary">Submit</button>
        </form> 
       </div> 
