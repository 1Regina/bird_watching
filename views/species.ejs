    <%- include('navigator_bar') -%>
    <div class="container">
         <div class="container">
             <h1>Counter of Species Notes</h1>
         </div>
     <br>
     <br>
    <div class="container-sm  d-flex flex-row">
    <div class="shadow p-3 mb-5 bg-white rounded">
      <%for (let i = 0; i< data.length ; i += 1) {%>
       
        <ul> 
           <div class="row">
             <div class="col-sm-10">
            <% let url = `/species/${data[i].species_id}` %>
            <h5>
              <a href="<%=url %>"><%=`${data[i].name}`%></a>
            </h5>  
             </div>
             <div class="col-sm-2">
              <h6>   
                <%=`${data[i].count}`%>
              </h6>
             </div>
           </div>
        </ul>
     
      <%}%>    
    </div> 
    </div>
    </div>  
