   <%- include('navigator_bar') -%>
    <div class="container capitalize">
        <h1>Edit Sighting </h1> 
         <br>
      
      <form action="/note/<%=oneNote.id%>/edit?_method=PUT" method="POST">
        <% for (const [key,value] of Object.entries(oneNote)) {%> 
          <% if (!(key==="OBSERVED")){%>
            <% if (!(key==="id") && !(key==="creator_id") && !(key === "species") && !(key==="email")  ) {%>
              <label for="<%= key %>"><h6><%= key %></h6></label><br />
              <input type="text" id="<%key%>" name="<%=key%>" value="<%=value%>"  
           
            <%} else if (key === "species") {%>
              <label for="species" class="form-label "><h5>Species:</h5></label>
                <div class = "w-50">
                   <select class="custom-select" id="species" name = "species" required>
                    <option selected><%=oneNote.species%></option>
                    <% birdName.forEach((species) => {%>
                      <option value = "<%= species.name %>"><%= species.name %></option>
                    <% }); %> 
                   </select>
                </div>    
                <br>
               <button type="submit" class="btn btn-secondary">Submit</button>
            <%}%>    
             <br>  
             <br> 
             <br>
          <%}else{%>
              <textarea id="observed" rows="10" cols="70" name="OBSERVED"></textarea><br> <br>
          <%}%>
        <%}%>

        <!-- <input type="submit" value="Submit" /> -->
       
      </form>
    </div>
 </body>
</html>